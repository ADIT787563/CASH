# Message Queue System Environment Variables
# Add these to your .env.local file

# WhatsApp API Configuration
WHATSAPP_API_URL=https://graph.facebook.com/v18.0/YOUR_PHONE_NUMBER_ID/messages
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token_here
WHATSAPP_APP_SECRET=your_meta_app_secret_here
WHATSAPP_VERIFY_TOKEN=your_random_verify_token_here

# Worker Security
WORKER_SECRET=your_random_worker_secret_here

# Example values (replace with your actual values):
# WHATSAPP_API_URL=https://graph.facebook.com/v18.0/123456789/messages
# WHATSAPP_ACCESS_TOKEN=EAAxxxxxxxxxxxxxxx
# WHATSAPP_APP_SECRET=abc123def456ghi789
# WHATSAPP_VERIFY_TOKEN=my_secure_verify_token_12345
# WORKER_SECRET=worker_secret_xyz789abc123
